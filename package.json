{
  "name": "mydiary",
  "engines": {
    "node": "10",
    "npm": "6.12.1"
  },
  "version": "1.0.0",
  "description": "MyDiary is an online journal where users can pen down their thoughts and feelings.",
  "main": "index.js",
  "scripts": {
    "query": "babel-node ./server/models/db.js createTables",
    "start": "npm run query && babel-node server/index.js",
    "dev": "nodemon --exec babel-node server/index.js",
    "test": "nyc --reporter=html --reporter=text mocha --require @babel/register --require @babel/polyfill --timeout 500 server/test/ --exit",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ishoborabyose/MyDiary.git"
  },
  "keywords": [
    "MyDiary"
  ],
  "author": "Ishoborabyose Beatrice",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ishoborabyose/MyDiary/issues"
  },
  "homepage": "https://github.com/ishoborabyose/MyDiary#readme",
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/polyfill": "^7.6.0",
    "@hapi/joi": "^16.1.7",
    "babel-node": "0.0.1-security",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "chai-http": "^4.3.0",
    "coveralls": "^3.0.7",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "nyc": "^14.1.1",
    "pg": "^7.12.1",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/node": "^7.6.3",
    "@babel/preset-env": "^7.6.3",
    "@babel/register": "^7.6.2",
    "@babel/runtime": "^7.6.3",
    "chai": "^4.2.0",
    "eslint": "^6.6.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "mocha": "^6.2.2",
    "nodemon": "^1.19.4"
  }
}
